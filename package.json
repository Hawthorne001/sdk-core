{
  "name": "@baseswapfi/sdk-core",
  "version": "1.0.4",
  "license": "MIT",
  "description": "⚒️ An SDK for building applications on top of Baseswap V3",
  "author": "0xShoegazer",
  "module": "dist/sdk.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "contracts:compile": "typechain --target ethers-v5 --out-dir src/abis/types './node_modules/@uniswap/v3-periphery/artifacts/contracts/**/*Multicall*.json'",
    "compile-v3-types": "npx typechain --target ethers-v5 --out-dir src/smart-order-router/types/v3 './node_modules/@uniswap/v3-periphery/artifacts/contracts/**/*.json'",
    "compile-v2-types": "npx typechain --target ethers-v5 --out-dir src/smart-order-router/types/v2 './node_modules/@uniswap/?(v2-core|v2-periphery)/build/*UniswapV2*.json'",
    "compile-router": "npx typechain --target ethers-v5 --out-dir src/smart-order-router/types/other './node_modules/@uniswap/swap-router-contracts/artifacts/contracts/**/*.json'",
    "compile-narwhal": "npx typechain --target ethers-v5 --out-dir src/smart-order-router/types/other './node_modules/@uniswap/narwhal/artifacts/contracts/**/*.json'",
    "compile-external-types": "npx typechain --target ethers-v5 --out-dir src/smart-order-router/types/other 'src/abis/**/*.json'",
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.0.12",
    "@ethersproject/address": "^5.0.2",
    "@ethersproject/contracts": "^5",
    "@ethersproject/providers": "5",
    "@ethersproject/solidity": "^5.0.9",
    "@reduxjs/toolkit": "1",
    "@typechain/ethers-v5": "^7.2.0",
    "@uniswap/router-sdk": "^1.6.0",
    "@uniswap/swap-router-contracts": "^1.3.0",
    "@uniswap/v2-sdk": "^3.2.0",
    "@uniswap/v3-periphery": "^1.3.0",
    "big.js": "^5.2.2",
    "bunyan": "^1.8.15",
    "bunyan-blackhole": "^1.1.1",
    "decimal.js-light": "^2.5.0",
    "ethers": "^5.4.7",
    "jsbi": "^3.1.4",
    "lodash": "^4.17.21",
    "react": ">=17",
    "react-redux": ">=7",
    "tiny-invariant": "^1.1.0",
    "toformat": "^2.0.0",
    "typechain": "^5.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "@types/big.js": "^4.0.5",
    "@types/bunyan": "^1.8.6",
    "@types/bunyan-blackhole": "^0.2.2",
    "@types/lodash": "^4.14.197",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.1.6"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/sdk.esm.js",
      "limit": "10 KB"
    }
  ]
}
